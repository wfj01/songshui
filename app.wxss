    transform: scale(0.5) translateZ(0);
  }
  .aui-btn-row .aui-btn:after {
    height: 200%;
    -webkit-transform: scale(0.5) translateZ(0);
    transform: scale(0.5) translateZ(0);
  }
  .aui-btn-row.aui-btn-row-round:after {
    width: 200%;
