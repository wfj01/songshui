  .aui-btn-row:after {
    width: 300%;
    height: 300%;
    border-radius: 6px;
    -webkit-transform: scale(0.333) translateZ(0);
    transform: scale(0.333) translateZ(0);
  }
  .aui-btn-row .aui-btn:after {
    height: 300%;
    -webkit-transform: scale(0.333) translateZ(0);
    transform: scale(0.333) translateZ(0);
  }
  .aui-btn-row.aui-btn-row-round:after {
    width: 300%;
    height: 300%;
